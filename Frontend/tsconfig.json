{
  "compilerOptions": {
    "jsx": "react",
    "strict": false,
    "module": "ESNext",
    "target": "ES5",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,

    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noEmit": true
  }
}